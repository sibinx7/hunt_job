.dashboard-wrapper
  .row
    .small-12.columns
      .dashboard-inner.custom-container
        .row
          .small-12.medium-8.large-9.columns
            .dashboard-tab-wrapper
              %h3.p-title
                Latest feedbacks for your
              .rating-wrapper
                -(1..10).each_with_index do |feedbacks,index|
                  .each-rating
                    .inner-rating
                      .rating-profile-and-rating
                        .profile-img
                          = image_tag @user_rating.user.profile_image.url
                        .rating
                          - (1..@user_rating.rating).each_with_index do |rate,index|
                            %span.fa.fa-star
                      .rating-description-info
                        .description
                          .inner-description
                            = @user_rating.description.html_safe
                        .pointer
                        .more-info

- content_for :extra_css do
  = stylesheet_link_tag 'user_ratings'